snippet log
	logger.debug("RL: ${1}"${2});
snippet m
	${1:name}: function (${2}) {${3}
	},
snippet pr
	${1:class_name}.prototype.${2:method_name} = function (${3}) {${4}
	};
snippet if
	if (${1:true}) {${2}
	}
# if ... else
snippet ife
	if (${1:true}) {${2}
	} else {${3}
	}
# for (...) {...}
snippet for
	for (var ${2:i} = 0; $2 < ${1:Things}.length; $2${3:++}) {
		${4:$1[$2]}
	}
